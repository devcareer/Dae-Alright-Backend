  "scripts": {
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel -d ./build ./src -s",
    "clean": "rm -rf .nyc_output build coverage && mkdir build",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo:all",
    "db:reset": "npm run db:migrate:undo && npm run db:migrate",
    "dev": "nodemon --exec babel-node ./src/index.js",
    "lint": "eslint \"**/*.js\"",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "start": "node ./build/index.js",
    "test": "cross-env NODE_ENV=test nyc mocha ./src/test -r @babel/register --recursive -t 1000 --exit"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "bcrypt": "^3.0.6",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "pg": "^7.12.1",
    "sequelize": "^5.21.1",
    "sequelize-cli": "^5.5.1",
    "swagger-ui-express": "^4.1.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "cross-env": "^6.0.3",
    "editorconfig": "^0.15.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2"
  }
}
